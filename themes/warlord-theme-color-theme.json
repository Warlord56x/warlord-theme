  "tokenColors": [
    {
      "name": "Token Error",
      "scope": "token.error-token",
      "settings": {
        "foreground": "#C8102E"
      }
    },
    {
      "name": "Token Warning",
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#F5DF4Dcc"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#F58C5895"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
      "settings": {
        "foreground": "#F58C5895",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#F58C58"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F58C58"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#F2F0EB"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#47A23F"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#A6192E"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#C8102E",
        "fontStyle": "bold"
        /*F58C58*/
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#47A23F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type",
      "settings": {
        "foreground": "#A6192E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#A6192E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#939597",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#A6192E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property, meta.field.declaration entity.name.function",
      "settings": {
        "foreground": "#A6192E"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#A6192E"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#A6192E"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#C8102E"
      }
    },
    {
      "name": "Reset embedded/template expression colors",
      "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
      "settings": {
        "foreground": "#F2F0EB"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#47A23F"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#47A23F"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#C8102E"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#C8102E"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#47A23F"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#A6192E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS property",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#47A23F90",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#C8102E"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#F2F0EB"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#A6192E"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#C8102E"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#47A23F",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#C8102E"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#47A23F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#A6192E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#47A23F",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown code block",
